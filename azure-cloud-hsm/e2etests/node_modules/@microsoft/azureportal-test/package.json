{
  "name": "@microsoft/azureportal-test",
  "version": "14.287.0",
  "description": "Microsoft Azure Portal Test Framework",
  "contributors": [],
  "types": "typescript/microsoft-azureportal-test.d.ts",
  "exports": {
    ".": "./lib/Index.js",
    "./*": "./lib/*.js"
  },
  "dependencies": {
    "@axe-core/playwright": "^4.9.1",
    "@azure/arm-resources": "~5.0.1",
    "@azure/identity": "^4.2.1",
    "@types/debug": "^4.1.5",
    "@types/findup-sync": "2.0.2",
    "@types/nconf": "0.10.2",
    "@types/selenium-webdriver": "^3.0.13",
    "axe-core": "^4.9.1",
    "axe-webdriverjs": "^2.3.0",
    "debug": "^4.1.1",
    "ejs": "^3.1.10",
    "findup-sync": "^4.0.0",
    "html-minifier-terser": "^7.1.0",
    "jsonwebtoken": "^9.0.0",
    "nconf": "^0.12.0",
    "playwright": "^1.44.1",
    "selenium-webdriver": "3.6.0",
    "v8-to-istanbul": "^9.2.0"
  },
  "directories": {
    "doc": "docs",
    "test": "test"
  },
  "devDependencies": {
    "@types/ejs": "^3.1.4",
    "@types/html-minifier-terser": "^7.0.0",
    "@types/jsonwebtoken": "^9.0.0",
    "@types/mocha": "^10.0.0",
    "@types/node": "^18.19.34",
    "chalk": "^4.0.0",
    "chromedriver": "121.0.0",
    "dts-bundle": "~0.7.3",
    "gulp": "^4.0.2",
    "json5": "^2.2.3",
    "mocha": "^10.1.0",
    "ts-node": "^8.10.2",
    "typedoc": "^0.25.13",
    "typescript": "~5.4.5",
    "v8-coverage-merge": "^1.1.2",
    "yargs": "^3.9.1"
  },
  "peerDependencies": {
    "@types/node": "^16.18.50 || ^18.16.16 || ^20.0.0 || ^22.0.0"
  },
  "repository": {
    "type": "git",
    "url": "https://msazure.visualstudio.com/DefaultCollection/One/_git/AzureUX-PortalFx"
  },
  "license": "MIT",
  "homepage": "https://aka.ms/portalfx/msportalfx-test",
  "keywords": [
    "Azure Portal",
    "test framework",
    "testing",
    "test",
    "selenium",
    "webdriver"
  ],
  "bugs": {
    "url": "https://aka.ms/portalfx/bug"
  },
  "scripts": {
    "build:test": "cd ./test & tsc -p tsconfig.json",
    "build:watch": "tsc -w -p tsconfig.json",
    "build": "tsc -p tsconfig.json & pnpm run bundle",
    "bundle": "pnpm dlx dts-bundle@0.7.3 --configJson ./dts-bundle.json",
    "clean": "git clean -xdf -e .env",
    "docs": "pnpm dlx gulp@4.0.2 readme --out",
    "docsite": "pnpm run docs & typedoc -out generated --mode modules ./src",
    "lint": "eslint ./**/*.ts",
    "test": "node_modules\\.bin\\mocha test\\. --full-trace --configJson test\\config.json",
    "testbrk": "node_modules\\.bin\\mocha --inspect-brk test\\. --full-trace --configJson test\\config.json",
    "testbrkgrep": "node_modules\\.bin\\mocha --inspect-brk test\\. --full-trace --configJson test\\config.json --grep",
    "testcx": "cd objd/amd64 & node_modules\\.bin\\mocha test\\. --full-trace --configJson test\\config.json",
    "testd": "mocha --inspect-brk test\\. --full-trace --configJson test\\config.json",
    "testgrep": "node_modules\\.bin\\mocha test\\. --full-trace --configJson test\\config.json --grep",
    "testjenkins": "mocha test\\. --configJson test\\config.json --reporter mocha-jenkins-reporter",
    "playwright": "playwright install"
  }
}
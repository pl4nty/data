{
    "compileOnSave": true,
    "compilerOptions": {
        "lib": [
            "dom",
            "ES2021",
        ],
        "module": "commonjs",
        "noImplicitAny": true,
        "noImplicitThis": true,
        "noUnusedLocals": true,
        "noUnusedParameters": true,
        "sourceMap": true,
        "target": "ES2021",
        "skipLibCheck": true,
        "typeRoots": [
            "node_modules/@types",
        ]
    },
    "exclude": [
        "objd"
    ]
}

{
  "$schema": "https://ev2schema.azure.net/schemas/2020-01-01/scopeBindings.json",
  "contentVersion": "0.0.0.1",
  "scopeBindings": [
    {
      "scopeTagName": "None",
      "bindings": [
        {
          "find": "__SUFFIX__",
          "replaceWith": ""
        }
      ]
    },
    {
      "scopeTagName": "NonProd",
      "bindings": [
        {
          "find": "__SUFFIX__",
          "replaceWith": "-NonProd"
        }
      ]
    },
    {
      "scopeTagName": "Prod",
      "bindings": [
        {
          "find": "__SUFFIX__",
          "replaceWith": "-Prod"
        }
      ]
    }
  ]
}

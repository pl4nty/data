{
  "layout": [
    {
      "build-num.txt": "electron windows x64/build id.txt"
    },
    {
      "chromedriver-v${version}-${platform}-${arch}.zip": "electron ${platform} ${arch}/chromedriver.zip"
    },
    {
      "dependencies.txt": "electron windows x64/dependencies.txt"
    },
    {
      "dump_syms-v${version}-${platform}-${arch}.zip": "electron ${platform} ${arch}/dump_syms.zip",
      "__filter__": {
        "arch": "x64",
        "platform": ["linux", "windows"]
      }
    },
    {
      "electron-api.json": "electron windows x64/electron-api.json"
    },
    {
      "electron-v${version}-${platform}-${arch}.zip": "electron ${platform} ${arch}/dist.zip"
    },
    {
      "electron-v${version}-${platform}-${arch}-dbg.zip": "electron ${platform} ${arch}/electron-dbg.zip",
      "__filter__": {"platform": "linux"}
    },
    {
      "electron-v${version}-${platform}-${arch}-dsym.zip": "electron ${platform} ${arch}/dsym.zip",
      "__filter__": {"platform": "mac"}
    },
    {
      "electron-v${version}-${platform}-${arch}-pdb.zip": "electron ${platform} ${arch}/pdb.zip",
      "__filter__": {"platform": "windows"}
    },
    {
      "electron-v${version}-${platform}-${arch}-symbols.zip": "electron ${platform} ${arch}/symbols.zip"
    },
    {
      "electron-v${version}-${platform}-${arch}-toolchain-profile.zip": "electron ${platform} ${arch}/toolchain_profile.zip",
      "__filter__": {"platform": "windows"}
    },
    {
      "electron.d.ts": "electron windows x64/electron.d.ts"
    },
    {
      "ffmpeg-v${version}-${platform}-${arch}.zip": "electron ${platform} ${arch}/ffmpeg.zip"
    },
    {
      "flavour.txt": "electron windows x64/flavour.txt"
    },
    {
      "hunspell_dictionaries.zip ": "electron windows x64/hunspell_dictionaries.zip"
    },
    {
      "mksnapshot-v${version}-${platform}-${arch}.zip": "electron ${platform} ${arch}/mksnapshot.zip"
    },
    {
      "third-party_attributions.txt": "electron windows x64/third-party_attributions.txt"
    },
    {
      "version.txt": "electron windows x64/version.txt"
    }
  ],
  "mapping": {
    "platform": {
      "mac": "darwin",
      "windows": "win32"
    },
    "arch": {
      "arm": "armv7l",
      "x86": "ia32"
    }
  },
  "checksums": [
    {
      "algorithm": "sha256",
      "as_binary": true,
      "output": "SHASUMS256.txt",
      "include_file_paths": false
    }
  ]
}
